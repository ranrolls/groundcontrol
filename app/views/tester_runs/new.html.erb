<h1>New tester_run</h1>

<%= error_messages_for :tester_run %>

<% form_for(@tester_run) do |f| %>
  <p>
    <b>Results data</b><br />
    <%= f.text_area :results_data %>
  </p>

  <p>
    <b>Tester</b><br />
    <%= f.text_field :tester_id %>
  </p>

  <p>
    <b>Results</b><br />
    <%= f.text_field :results_id %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', tester_runs_path %>
