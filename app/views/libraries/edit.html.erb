<h1>Editing library</h1>

<%= error_messages_for :library %>

<% form_for(@library) do |f| %>
  <p>
    <b>Name</b><br />
    <%= f.text_field :name %>
  </p>

  <p>
    <b>Code</b><br />
    <%= f.text_area :code %>
  </p>

  <p>
    <b>Project</b><br />
    <%= f.text_field :project_id %>
  </p>

  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @library %> |
<%= link_to 'Back', libraries_path %>
